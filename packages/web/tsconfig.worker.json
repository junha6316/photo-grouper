    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": false,
    "skipLibCheck": true,
    "strict": true,
    "outDir": "./dist",
    "noEmit": false,
    "types": ["@cloudflare/workers-types", "./worker-configuration.d.ts"]
  },
  "include": [
    "./worker-configuration.d.ts",
    "./_worker.ts",
    "./_worker-utils.ts"
  ]
}
